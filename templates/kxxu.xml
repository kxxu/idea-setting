<templateSet group="kxxu">
  <template name="sl4j" value="private static final Logger log = LoggerFactory.getLogger($classname$.class);" description="sl4j log" toReformat="false" toShortenFQNames="true">
    <variable name="classname" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="sfs" value="static final String " description="static final string" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_COMMENT" value="false" />
      <option name="JAVA_EXPRESSION" value="false" />
    </context>
  </template>
  <template name="psfs" value="public static final String " description="public static final string" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_COMMENT" value="false" />
    </context>
  </template>
  <template name="todo" value="// TODO " description="todo" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="GO" value="true" />
      <option name="GROOVY" value="true" />
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ins" value="if(!Strings.isNullOrEmpty($VAR$)){&#10;  $END$&#10;}" description="if string" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="suggestFirstVariableName()" defaultValue="var" alwaysStopAt="true" />
    <context>
      <option name="GROOVY" value="true" />
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>